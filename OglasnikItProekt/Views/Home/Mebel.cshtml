@model OglasnikItProekt.Models.Mebel
@{
    ViewBag.Title = "Laptop";
}

<h2 class="text-center pb-3" style="color: darkblue">Мебел</h2>
<div class="col-sm-4 col-lg-4 col-md-4">
    <div class="thumbnail">
        <img src="/Images/k/m1.jpg" alt="" style="width:250px; height:200px" />
        <div class="caption">
            @*<h3 class="pull-right">$@Model.Cena.ToString()</h3>*@
            <h3>
                <a href="/Home/Details/@Model.MebelID">@Model.Ime</a>
            </h3>
            <p>@Model.Opis</p>
        </div>
        <div class="addToCart text-right">
            <p class="button ">
                <a>@Html.ActionLink("Види повеќе..", "AddToShoppingCart", "ShoppingCart", new { id = Model.MebelID }, new { @class = "btn btn-success" })</a>
            </p>
        </div>

    </div>

</div>
