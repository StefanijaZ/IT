@model OglasnikItProekt.Models.Zivealiste
@{
    ViewBag.Title = "Avtomobil";
}

<h2 class="text-center pb-3" style="color: darkblue">Живеалишта</h2>
<div class="col-sm-4 col-lg-4 col-md-4">
    <div class="thumbnail">
        <img src="/Images/z/z1.jpg" alt="" style="width:350px; height:200px" />
        <div class="caption">
            @*<h3 class="pull-right">$@Model.Cena.ToString()</h3>*@
            <h3>
                <a href="/Home/Details/@Model.ZivealisteID">@Model.Ime</a>
            </h3>
            <p>@Model.Opis</p>
        </div>
        <div class="addToCart text-right">
            <p class="button ">
                <a>@Html.ActionLink("Види повеќе..", "AddToShoppingCart", "ShoppingCart", new { id = Model.ZivealisteID }, new { @class = "btn btn-success" })</a>
            </p>
        </div>

    </div>

</div>
